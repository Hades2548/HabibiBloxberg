<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 800 600">
  <defs>
    <radialGradient id="skyGradient" cx="50%" cy="30%" r="70%">
      <stop offset="0%" style="stop-color:#f0f0f0;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#d0d0d0;stop-opacity:1" />
    </radialGradient>
    <filter id="blur" x="-50%" y="-50%" width="200%" height="200%">
      <feGaussianBlur in="SourceGraphic" stdDeviation="2"/>
    </filter>
  </defs>
  
  <!-- Sky background -->
  <rect width="800" height="600" fill="url(#skyGradient)"/>
  
  <!-- Clouds -->
  <g opacity="0.8" filter="url(#blur)">
    <ellipse cx="150" cy="100" rx="60" ry="25" fill="#ffffff" opacity="0.7"/>
    <ellipse cx="180" cy="95" rx="40" ry="20" fill="#ffffff" opacity="0.6"/>
    <ellipse cx="120" cy="105" rx="35" ry="15" fill="#ffffff" opacity="0.8"/>
    
    <ellipse cx="400" cy="80" rx="80" ry="30" fill="#ffffff" opacity="0.6"/>
    <ellipse cx="440" cy="75" rx="50" ry="25" fill="#ffffff" opacity="0.7"/>
    <ellipse cx="360" cy="85" rx="45" ry="20" fill="#ffffff" opacity="0.5"/>
    
    <ellipse cx="650" cy="120" rx="70" ry="28" fill="#ffffff" opacity="0.7"/>
    <ellipse cx="680" cy="115" rx="45" ry="22" fill="#ffffff" opacity="0.6"/>
  </g>
  
  <!-- Floating particles -->
  <g opacity="0.4">
    <circle cx="100" cy="200" r="2" fill="#ffffff"/>
    <circle cx="250" cy="150" r="1.5" fill="#ffffff"/>
    <circle cx="400" cy="180" r="2.5" fill="#ffffff"/>
    <circle cx="550" cy="160" r="1" fill="#ffffff"/>
    <circle cx="700" cy="190" r="2" fill="#ffffff"/>
    <circle cx="150" cy="250" r="1.5" fill="#ffffff"/>
    <circle cx="350" cy="220" r="1" fill="#ffffff"/>
    <circle cx="600" cy="240" r="2" fill="#ffffff"/>
  </g>
  
  <!-- Trees silhouettes -->
  <g opacity="0.9">
    <!-- Tree 1 -->
    <g transform="translate(50, 350)">
      <rect x="18" y="80" width="14" height="120" fill="#4a4a4a"/>
      <ellipse cx="25" cy="80" rx="45" ry="60" fill="#6a6a6a" opacity="0.8"/>
      <ellipse cx="20" cy="70" rx="35" ry="45" fill="#7a7a7a" opacity="0.7"/>
      <ellipse cx="30" cy="75" rx="40" ry="50" fill="#5a5a5a" opacity="0.9"/>
    </g>
    
    <!-- Tree 2 -->
    <g transform="translate(200, 320)">
      <rect x="22" y="100" width="16" height="150" fill="#3a3a3a"/>
      <ellipse cx="30" cy="100" rx="55" ry="70" fill="#6a6a6a" opacity="0.8"/>
      <ellipse cx="25" cy="85" rx="40" ry="55" fill="#7a7a7a" opacity="0.7"/>
      <ellipse cx="35" cy="90" rx="45" ry="60" fill="#5a5a5a" opacity="0.9"/>
      <ellipse cx="30" cy="75" rx="30" ry="40" fill="#8a8a8a" opacity="0.6"/>
    </g>
    
    <!-- Tree 3 -->
    <g transform="translate(600, 340)">
      <rect x="20" y="90" width="15" height="130" fill="#4a4a4a"/>
      <ellipse cx="27" cy="90" rx="50" ry="65" fill="#6a6a6a" opacity="0.8"/>
      <ellipse cx="22" cy="80" rx="38" ry="50" fill="#7a7a7a" opacity="0.7"/>
      <ellipse cx="32" cy="85" rx="42" ry="55" fill="#5a5a5a" opacity="0.9"/>
    </g>
    
    <!-- Tree 4 -->
    <g transform="translate(450, 360)">
      <rect x="16" y="70" width="12" height="110" fill="#4a4a4a"/>
      <ellipse cx="22" cy="70" rx="40" ry="55" fill="#6a6a6a" opacity="0.8"/>
      <ellipse cx="18" cy="60" rx="30" ry="40" fill="#7a7a7a" opacity="0.7"/>
      <ellipse cx="26" cy="65" rx="35" ry="45" fill="#5a5a5a" opacity="0.9"/>
    </g>
  </g>
  
  <!-- Ground/horizon -->
  <rect x="0" y="480" width="800" height="120" fill="#e0e0e0" opacity="0.3"/>
</svg>